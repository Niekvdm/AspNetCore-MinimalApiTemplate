{
	"Serilog": {
		"MinimumLevel": {
			"Default": "Information",
			"Override": {
				"Microsoft.AspNetCore": "Warning",
				"Microsoft.EntityFrameworkCore.Database.Command": "Warning"
			}
		},
		"WriteTo": [
			"Console"
		]
	},
	"AllowedHosts": "*",
	"Kestrel": {
		"AllowedOrigins": [
			"http://127.0.0.1"
		]
	}
}
